{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Debug Join",
  "width": 500,
  "height": 400,
  "data": {
    "url": "ne_10m.json",
    "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {"url": "TotalPopAus_2024.csv"},
        "key": "State",
        "fields": ["Total"]
      }
    }
  ],
  "mark": "geoshape",
  "encoding": {
    "color": {"field": "Total", "type": "quantitative"}
  }
}
